{
  "name": "summit-kit",
  "version": "0.6.2",
  "type": "module",
  "module": "dist/esm/summit-kit.esm.js",
  "main": "dist/cjs/summit-kit.cjs",
  "types": "dist/lib/index.d.ts",
  "scripts": {
    "dev": "vite --config vite.site.config.ts",
    "build": "npm run build:lib && npm run build:site",
    "build:lib": "tsc -b && vite build --config vite.lib.config.ts",
    "build:site": "tsc -b && vite build --config vite.site.config.ts",
    "check": "npx @biomejs/biome check --write src lib",
    "preview": "vite preview",
    "prepare": "husky"
  },
  "dependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.4.0",
    "react-syntax-highlighter": "^15.6.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.13.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "react-router": "^7.1.5",
    "typescript": "^5.7.3",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-pwa": "^0.21.1",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
